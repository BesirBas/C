#include <stdio.h>
int i,j;

int main(){

	printf("********************************");
	printf("**\n**      CARPIM TABLOSU          **\n");
	printf("**********************************\n");
	for (i=1; i<=10 ; i++){
		if(i==6|| i==9||i==10) printf("%d LAR \n");
		 else printf("\n%d LER\n",i);
		  
		 for (j=1; j<10; j++)
		 printf("%d x %d = %d\n",i ,j ,i*j);
		 printf("**********************************\n");
}			
}
